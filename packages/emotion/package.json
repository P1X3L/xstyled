{
  "name": "@wttj/xstyled-emotion",
  "description": "A utility-first CSS-in-JS framework built for React (Emotion bindings).",
  "version": "3.8.0",
  "keywords": [
    "emotion",
    "css",
    "system",
    "styled-system",
    "tailwindcss"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "author": "Greg Berg√© <berge.greg@gmail.com>",
  "license": "MIT",
  "scripts": {
    "reset": "rm -rf dist",
    "build": "../../node_modules/.bin/rollup -c ../../build/rollup.config.js",
    "prepublishOnly": "npm run reset && npm run build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/wttj"
  },
  "homepage": "https://xstyled.dev",
  "repository": "https://github.com/P1X3L/xstyled/tree/core-upgrade-styled-components-v6/packages/emotion",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/gregberge"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "dependencies": {
    "@wttj/xstyled-core": "^3.8.0",
    "@wttj/xstyled-system": "^3.8.0",
    "@wttj/xstyled-util": "^3.7.0"
  }
}
